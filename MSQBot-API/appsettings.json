{
  "https_port": 443,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MSQBotDb": "$",
    { "MsqbotDatabaseConnectionString" }
  },
  "JWT": {
    "IssuerSigningKey": "$",
    { "issuerSigningKey" },
    "ValidateIssuerSigningKey": true,
    "ValidateIssuer": true,
    "ValidIssuer": "https://localhost:7168",
    "ValidateAudience": true,
    "ValidAudience": "https://localhost:7168",
    "RequireExpirationTime": true,
    "ValidateLifetime": true
  },
  "DiscordOauth": {
    "token": "$",
    { "discordAuthToken" },
    "clientId": "$",
    { "discordClientId" }
  }
}